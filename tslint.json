{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-no-unused-expression-chai"
  ],
  "jsRules": {},
  "rules": {
    "radix": false,
    "semicolon": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "quotemark": [true, "single", "jsx-double"],
    "curly": [true, "ignore-same-line"]
  },
  "linterOptions": {
    "exclude": []
  }
}